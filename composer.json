{
    "name": "macellan/laravel-excel",
    "description": "Supercharged Excel exports in Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "phpexcel",
        "excel",
        "csv",
        "export",
        "import",
        "batch"
    ],
  "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/syntaxevolution/PHPExcel"
        }
    ],
    "authors": [
        {
            "name": "Maatwebsite.nl",
            "email": "patrick@maatwebsite.nl"
        },
        {
            "name": "Fatih Aytekin",
            "email": "fatih@aytekin.me"
        }
    ],
    "require": {
        "php": ">=5.5",
        "macellan/phpexcel": "^1.9.2",
        "illuminate/cache": ">=5.0",
        "illuminate/config": ">=5.0",
        "illuminate/filesystem": ">=5.0",
        "illuminate/support": ">=5.0",
        "jeremeamia/superclosure": ">=2.3",
        "nesbot/carbon": ">=2.17",
        "tijsverkoyen/css-to-inline-styles": "~2.2",
        "ext-dom": "*",
        "ext-simplexml": "*"
    },
    "suggest": {
        "illuminate/http": ">=5.0",
        "illuminate/routing": ">=5.0",
        "illuminate/view": ">=5.0",
        "illuminate/queue": ">=5.0"
    },
    "autoload": {
        "classmap": [
            "src/Macellan/Excel"
        ],
        "psr-0": {
            "Macellan\\Excel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Macellan\\Excel\\ExcelServiceProvider"
            ],
            "aliases": {
                "XLS": "Macellan\\Excel\\Facades\\Excel"
            }
        }
    }
}
